{
    "env": {
        "node": true
    },
    "extends": [
        "airbnb/base",
        "prettier",
        "plugin:node/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-underscore-dangle": "off",
        "prettier/prettier": "off",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "camelcase": "off"
    }
}